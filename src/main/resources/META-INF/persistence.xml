<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">

        <class>com.gift.giftproject.model.DocumentsEntity</class>
        <class>com.gift.giftproject.model.EvaluationsEntity</class>
        <class>com.gift.giftproject.model.InternshipEntity</class>
        <class>com.gift.giftproject.model.StudentEntity</class>
        <class>com.gift.giftproject.model.TutorEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:postgresql://localhost:5432/GIFTProject"/>
            <property name="eclipselink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="eclipselink.jdbc.user" value="admin"/>
            <property name="eclipselink.jdbc.password" value="root"/>
        </properties>
    </persistence-unit>
</persistence>
